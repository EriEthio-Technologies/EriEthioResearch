{
  "version": 2,
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/web-vitals",
      "dest": "/api/web-vitals",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
} 